rv32ui_sc_tests =    \
    simple \
    add addi \
    and andi \
    auipc \
    beq bge bgeu blt bltu bne \
    fence_i \
    jal jalr \
    lb lbu lh lhu lw \
	lui \
    or ori \
	sb sh sw \
	sll slli \
	slt slti sltiu sltu \
	sra srai \
	srl srli \
	sub \
	xor xori

rv32ui_tests = $(addsuffix .elf, $(rv32ui_sc_tests))

target_tests += $(rv32ui_tests)
